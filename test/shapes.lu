export Point, Point3D, answer, cheese

class Point {
   has x = 0
   has y = 0
   method move(x, y) {
      self.x = x
      self.y = y
   }
}

class Point3D from Point {
   has z = 0
   method move(x, y, z) {
      super.move(x, y)
      self.z = z
   }
}

